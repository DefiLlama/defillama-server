{
  "name": "price-server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "deploy:prod": "sls deploy --stage prod",
    "format": "prettier --write \"src/**/*.ts\"",
    "serve": "node --max-old-space-size=8192 node_modules/serverless/bin/serverless offline start",
    "test": "jest",
    "test:watch": "jest --watch",
    "refill": "tableName='prod-coins-table' AWS_REGION='eu-central-1' ts-node src/refill",
    "build": "sls package"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.72",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.10",
    "aws-sdk": "^2.1150.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "esbuild": "^0.14.38",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "serverless": "^2.31.0",
    "serverless-esbuild": "^1.26.2",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-datadog": "^5.14.1",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-prune-plugin": "^1.4.4",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
